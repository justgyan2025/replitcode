{
    "version": 2,
    "builds": [
        {
            "src": "main.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "main.py"
        }
    ],
    "functions": {
        "api/*": {
            "memory": 1024,
            "maxDuration": 10
        }
    },
    "env": {
        "PIP_TARGET": "./.pip"
    }
}